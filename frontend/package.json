{
  "name": "@headless-wp/frontend",
  "version": "1.0.0",
  "description": "",
  "main": "server.js",
  "private": true,
  "scripts": {
    "dev": "node .",
    "build": "next build",
    "start": "cross-env NODE_ENV=production node .",
    "format": "prettier --write \"./**/*.{ts,tsx}\"",
    "lint": "tslint -p tsconfig.json \"./**/*.{ts,tsx}\"",
    "docker:build": "docker build -t frontend .",
    "docker:clean": "docker rm -f frontend || true",
    "docker:run": "docker run -p 3000:3000 --name frontend frontend",
    "docker:stop": "docker stop frontend",
    "docker:start": "docker start frontend && yarn run docker:logs",
    "docker:logs": "docker logs -f frontend",
    "deploy": "yarn run docker:build && yarn run docker:clean && yarn run docker:run"
  },
  "keywords": [],
  "author": "",
  "license": "GPL-2.0",
  "browserslist": [
    ">0.25%",
    "not ie 11",
    "not op_mini all"
  ],
  "dependencies": {
    "@wordpress/block-library": "^2.0.1",
    "@zeit/next-sass": "^1.0.1",
    "@zeit/next-typescript": "^1.1.1",
    "autoprefixer": "^9.1.5",
    "babel-plugin-module-resolver": "^3.1.1",
    "express": "^4.16.2",
    "glob": "^7.1.3",
    "isomorphic-unfetch": "^3.0.0",
    "modern-normalize": "^0.5.0",
    "next": "^7.0.1",
    "next-compose-plugins": "^2.1.1",
    "postcss-easy-import": "^3.0.0",
    "react": "^16.5.2",
    "react-dom": "^16.5.2",
    "typescript": "^3.1.1"
  },
  "devDependencies": {
    "@types/express": "^4.16.0",
    "@types/next": "^7.0.1",
    "@types/node": "^10.11.4",
    "@types/react": "^16.4.15",
    "@types/react-dom": "^16.0.8",
    "@types/styled-jsx": "^2.2.5",
    "cross-env": "^5.2.0",
    "prettier": "^1.14.3",
    "tslint": "^5.11.0",
    "tslint-config-blvd": "^1.2.1",
    "tslint-config-prettier": "^1.15.0",
    "tslint-plugin-prettier": "^2.0.0"
  }
}
